snd-soc-smdk6410-s5m8751-objs := smdk6410_s5m8751.o
snd-soc-smdk6440-s5m8751-objs := smdk6440_s5m8751.o

obj-$(CONFIG_SND_S3C64XX_SOC_SMDK6410_WM9713) += snd-soc-smdk64xx-wm9713.o
